@model IEnumerable<string>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    
}

    <h1 class="text-center">@ViewBag.Message</h1>

    
    @foreach (var item in Model)
    {
        <div class="row">
             <ul>
                  <li>@item</li>
              </ul>
        </div>
        <div class="row">
            <ul>
                <li>@item</li>
            </ul>
        </div>
        <div class="row">
            <ul>
                <li>@item</li>
            </ul>
        </div>
        <div class="row">
            <ul>
                <li>@item</li>
            </ul>
        </div>

    }
    

